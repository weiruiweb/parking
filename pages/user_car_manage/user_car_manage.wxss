page{
	min-height:100%;
	width:100%;
}
.user_banner{
	width:100%;
	height:350rpx;
}
.user_banner image{
	width:400rpx;
	height:124rpx;
	margin:0 auto;
}
.user_left{
	height:110rpx;
}
.user_left image{
	width:34rpx;
	height:36rpx;
	margin-right:12rpx;
}
.title_left{
	width:5%;
	text-align:center;
}
.title_right{
	width:95%;
}
.province{
	width:21%;
	height:72rpx;
	border: solid 1px #e0e0e0;
	border-radius: 10rpx;
}
.province input{
	width:100%;
	height:90%;
	border:none;
	text-align: center;
}
.circle{
	width:4.5%;
}
.number_car{
	width:10.5%;
	height:72rpx;
	border: solid 1px #e0e0e0;
	border-radius: 10rpx;
}
.number_car input{
	width:100%;
	height:100%;
	border:none;
	text-align: center;
}
.number_car>view{
	text-align: center;
}
#number{
	margin-bottom: 30rpx;
}
.submit_info{
	margin-top:140rpx;
}