/**index.wxss**/

@import "../../wxParse/wxParse.wxss"

page{
	width: 100%;
	min-height: 100%;
}
.user_banner{
	width:100%;
	height:294rpx;
	position:relative;
}
.user_banner image{
	width:100%;
	height:100%;
}
.user_box{
	position:absolute;
	left:0;
	top:0;
	height:294rpx;
	display:flex;
}
.banner_left{
	width:67%;
	height:100%;

}
.banner_right image{
	width:34rpx;
	height:36rpx;
	margin-right: 20rpx;
}
.banner_right{
	width:33%;
	height:100%;
}
.user_img{
	width:140rpx;
	height:140rpx;
	border-radius:50%;
	overflow:hidden;
	margin:0px auto;
	border:solid 6rpx #eb0096;
}
.user_left{
	align-items:center;
}
.user_left image{
	width:34rpx;
	height:36rpx;
	margin-right:12rpx;
}
.user_infor{
	height:96rpx;
	line-height:96rpx;
	background:#fff;
	border-bottom:solid 1px #dcdcdc;
	align-items:center;
	}
.next_page image{
	width:11rpx;
	height:20rpx;
}
.user_all{
	margin-top:12rpx;
}